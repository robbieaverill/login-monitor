{
    "name": "silverstripe/login-monitor",
    "description": "A backend module to monitor login attempts and perform actions",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "silverstripe/framework": "^4.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\LoginMonitor\\": "src/",
            "SilverStripe\\LoginMonitor\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
